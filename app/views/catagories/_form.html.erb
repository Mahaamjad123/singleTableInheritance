<%= form_with model: [@user, catagory], local: catagory do |form| %>
    <% if catagory.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(catagory.errors.count, "error") %> prohibited this catagory from being saved:</h2>
  
        <ul>
        <% catagory.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
        </ul>
      </div>
    <% end %>
  
    <div class="field">
      <%= form.label :Price %>
      <%= form.text_field :price, id: :catagory_price %>
    </div>
  
    <div class="field">
      <%= form.label :Color %>
      <%= form.text_field :color, id: :catagory_color %>
    </div>
  
    <div class="field">
      <%= form.label :Model %>
      <%= form.text_field :model, id: :catagory_model %>
    </div>
  
    <div class="field">
      <%= form.label :Company %>
      <%= form.text_field :company, id: :catagory_company %>
    </div>
  
    <div class="field">
      <%= form.label :city %>
      <%= form.text_field :city, id: :catagory_city %>
    </div>
  
  
  
    <div class="actions">
      <%= form.submit %>
    </div>
  <% end %>